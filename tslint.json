{
    "rules": {
        "class-name": true,
        "no-var-keyword": true,
        "no-internal-module": true,
        "no-null-keyword": true,
        "prefer-const": true,
        "jsdoc-format": true,
        "no-console": [false, "log"],
        "no-any": false
    },
    "extends": [
        "tslint:latest",
        "tslint-config-prettier"
    ]
}